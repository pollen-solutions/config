{
  "name": "pollen-solutions/config",
  "description": "Pollen Solutions - Config Component - Configuration contraint schema, load and dispatch.",
  "homepage": "https://www.presstify.com/pollen-solutions/config/",
  "keywords": [
    "pollen-solutions",
    "component",
    "config"
  ],
  "time": "2021-07-09",
  "license": "MIT",
  "authors": [
    {
      "name": "Jordy Manner",
      "email": "jordy@presstify.com"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "ext-json": "*",
    "league/config": "^1.0",
    "pollen-solutions/support": "^1.0",
    "symfony/yaml": "^5.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "roave/security-advisories": "dev-latest"
  },
  "autoload": {
    "psr-4": {
      "Pollen\\Config\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pollen\\Config\\Test\\": "tests"
    }
  },
  "suggest": {
    "pollen-solutions/container" : "Pollen Container Component - PSR-11 ready Dependencies Injection Container."
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
